        "tables": [
            {
                "projects": {
                    "id": "bigint not null primary key",
                    "path": "text not null",
                    "owner_path": "text not null",
                    "display_name": "text",
                    "description": "text",
                    "avatar": "text",
                    "stars": "int",
                    "forks": "int",
                    "owned_by_user": "boolean",
                    "commit_count": "bigint",
                    "storage_size": "bigint",
                    "repository_size": "bigint",
                    "lfs_objects_size": "bigint",
                    "archived": "boolean",
                    "issues_enabled": "boolean",
                    "merge_requests_enabled": "boolean",
                    "wiki_enabled": "boolean",
                    "jobs_enabled": "boolean",
                    "snippets_enabled": "boolean",
                    "ci_config_path": "text",
                    "created_at": "datetime",
                    "last_activity": "datetime",
                    "members_processed": "boolean default false"
                },
                "users": {
                    "id": "bigint not null primary key",
                    "name": "text not null",
                    "username": "text not null",
                    "avatar": "text",
                    "contributions_processed": "boolean default false"
                }
            },
            {
                "forks": {
                    "source": "bigint not null",
                    "destination": "bigint not null",
                    "constraint FK_fork_source": "foreign key (source) references projects(id)",
                    "constraint FK_fork_destination": "foreign key (destination) references projects(id)",
                    "constraint PK_forks": "primary key(source, destination)"
                },
                "membership": {
                    "user": "bigint not null",
                    "constraint FK_membership_user": "foreign key (user) references users(id)",
                    "project": "bigint not null",
                    "constraint FK_membership_project": "foreign key (project) references projects(id)",
                    "constraint PK_membership": "primary key(user, project)"
                },
                "contributions": {
                    "user": "bigint not null",
                    "constraint FK_contribution_user": "foreign key (user) references users(id)",
                    "project": "bigint not null",
                    "constraint FK_contribution_project": "foreign key (project) references projects(id)",
                    "constraint PK_contribution": "primary key(user, project)"
                }
            }
        ]
    },
